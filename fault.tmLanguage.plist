				</dict>
				<dict>
					<key>match</key>
					<string>[\(\)]</string>
					<key>name</key>
					<string>punctuation.section.parens.fault</string>
				</dict>
				<dict>
					<key>match</key>
					<string>[\{\}]</string>
					<key>name</key>
					<string>punctuation.section.braces.fault</string>
				</dict>
			</array>
		</dict>
		<key>identifiers</key>
		<dict>
			<key>patterns</key>
			<array>
				<dict>
					<key>match</key>
					<string>\b[a-zA-Z_][a-zA-Z0-9_]*\b</string>
					<key>name</key>
					<string>variable.other.fault</string>
				</dict>
			</array>
		</dict>
	</dict>
	<key>scopeName</key>
	<string>source.fault</string>
</dict>
