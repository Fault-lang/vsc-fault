				<dict>
					<key>match</key>
					<string>(==|!=|&lt;=|&gt;=|&lt;|&gt;)</string>
					<key>name</key>
					<string>keyword.operator.comparison.fault</string>
				</dict>
				<dict>
					<key>match</key>
					<string>(\*\*|\+\+|--|\+|-|\*|/|%|\^|&lt;&lt;|&gt;&gt;|&amp;\^|&amp;|\|)</string>
					<key>name</key>
					<string>keyword.operator.arithmetic.fault</string>
				</dict>
			</array>
		</dict>
		<key>punctuation</key>
