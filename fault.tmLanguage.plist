			<key>include</key>
			<string>#identifiers</string>
		</dict>
	</array>
	<key>repository</key>
	<dict>
		<key>comments</key>
		<dict>
			<key>patterns</key>
			<array>
				<dict>
					<key>match</key>
					<string>//.*$</string>
					<key>name</key>
					<string>comment.line.double-slash.fault</string>
				</dict>
				<dict>
					<key>begin</key>
					<string>/\*</string>
					<key>end</key>
					<string>\*/</string>
					<key>name</key>
					<string>comment.block.fault</string>
				</dict>
			</array>
		</dict>
		<key>keywords</key>
		<dict>
			<key>patterns</key>
			<array>
				<dict>
					<key>match</key>
					<string>\b(all|assert|assume|clock|const|def|else|flow|for|func|if|import|init|new|return|run|spec|stock|then|when|this|eventually|eventually-always|always|nmt|nft)\b</string>
					<key>name</key>
					<string>keyword.control.fault</string>
				</dict>
				<dict>
					<key>match</key>
					<string>\b(advance|component|choose|global|leave|system|start|states|stay)\b</string>
					<key>name</key>
					<string>keyword.other.fault</string>
				</dict>
				<dict>
					<key>match</key>
					<string>\b(nil|true|false|now)\b</string>
					<key>name</key>
					<string>constant.language.fault</string>
				</dict>
			</array>
		</dict>
		<key>types</key>
